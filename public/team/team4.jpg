<svg xmlns="http://www.w3.org/2000/svg" width="400" height="400" viewBox="0 0 400 400">
  <defs>
    <linearGradient id="grad1" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#2c3e50;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#3498db;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="400" height="400" fill="url(#grad1)"/>
  
  <!-- Profile picture placeholder -->
  <g transform="translate(100, 80)">
    <!-- Head and shoulders silhouette -->
    <circle cx="100" cy="100" r="80" fill="#ecf0f1"/>
    
    <!-- Abstract face -->
    <g transform="translate(60, 70)">
      <!-- Eyes -->
      <circle cx="30" cy="0" r="8" fill="#2c3e50"/>
      <circle cx="70" cy="0" r="8" fill="#2c3e50"/>
      
      <!-- Mouth -->
      <path d="M30,40 Q50,55 70,40" fill="none" stroke="#2c3e50" stroke-width="3" stroke-linecap="round"/>
      
      <!-- Nose -->
      <path d="M50,15 L45,30 L55,30 Z" fill="#2c3e50"/>
    </g>
    
    <!-- Hair (curly, medium length) -->
    <path d="M50,50 C40,40 40,30 50,20 C70,0 130,0 150,20 C160,30 160,40 150,50" fill="#2c3e50"/>
    <circle cx="60" cy="40" r="10" fill="#2c3e50"/>
    <circle cx="80" cy="30" r="10" fill="#2c3e50"/>
    <circle cx="100" cy="25" r="10" fill="#2c3e50"/>
    <circle cx="120" cy="30" r="10" fill="#2c3e50"/>
    <circle cx="140" cy="40" r="10" fill="#2c3e50"/>
    
    <!-- Shoulders/Dress -->
    <path d="M40,180 L70,150 L130,150 L160,180 L160,220 L40,220 Z" fill="#e74c3c"/>
    
    <!-- Neckline -->
    <path d="M80,150 Q100,170 120,150" fill="none" stroke="#e74c3c" stroke-width="5"/>
    
    <!-- Earrings -->
    <circle cx="50" cy="70" r="5" fill="#f1c40f"/>
    <circle cx="150" cy="70" r="5" fill="#f1c40f"/>
  </g>
  
  <!-- Name plate -->
  <g transform="translate(200, 320)" text-anchor="middle">
    <text font-family="Arial" font-size="24" fill="white" font-weight="bold">Olivia Rodriguez</text>
    <text font-family="Arial" font-size="16" fill="white" y="25">Interior Design Lead</text>
  </g>
</svg>