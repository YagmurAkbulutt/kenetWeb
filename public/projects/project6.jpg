<svg xmlns="http://www.w3.org/2000/svg" width="800" height="600" viewBox="0 0 800 600">
  <defs>
    <linearGradient id="grad1" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#16222a;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#3a6073;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="800" height="600" fill="url(#grad1)"/>
  
  <!-- Urban Mixed-Use Development -->
  <g transform="translate(50, 50)">
    <!-- Multiple buildings in a complex -->
    <g opacity="0.9">
      <!-- Tallest tower -->
      <rect x="300" y="0" width="150" height="400" fill="white"/>
      
      <!-- Medium tower -->
      <rect x="100" y="100" width="150" height="300" fill="white"/>
      
      <!-- Low-rise building -->
      <rect x="500" y="200" width="200" height="200" fill="white"/>
      
      <!-- Connecting podium -->
      <rect x="100" y="350" width="600" height="50" fill="white"/>
    </g>
    
    <!-- Windows pattern -->
    <g fill="#3498db" opacity="0.7">
      <!-- Tallest tower windows -->
      <g>
        <rect x="320" y="30" width="30" height="40"/>
        <rect x="370" y="30" width="30" height="40"/>
        <rect x="320" y="90" width="30" height="40"/>
        <rect x="370" y="90" width="30" height="40"/>
        <rect x="320" y="150" width="30" height="40"/>
        <rect x="370" y="150" width="30" height="40"/>
        <rect x="320" y="210" width="30" height="40"/>
        <rect x="370" y="210" width="30" height="40"/>
        <rect x="320" y="270" width="30" height="40"/>
        <rect x="370" y="270" width="30" height="40"/>
        <rect x="320" y="330" width="30" height="40"/>
        <rect x="370" y="330" width="30" height="40"/>
      </g>
      
      <!-- Medium tower windows -->
      <g>
        <rect x="120" y="130" width="30" height="40"/>
        <rect x="170" y="130" width="30" height="40"/>
        <rect x="120" y="190" width="30" height="40"/>
        <rect x="170" y="190" width="30" height="40"/>
        <rect x="120" y="250" width="30" height="40"/>
        <rect x="170" y="250" width="30" height="40"/>
        <rect x="120" y="310" width="30" height="40"/>
        <rect x="170" y="310" width="30" height="40"/>
      </g>
      
      <!-- Low-rise building windows -->
      <g>
        <rect x="520" y="230" width="40" height="30"/>
        <rect x="580" y="230" width="40" height="30"/>
        <rect x="640" y="230" width="40" height="30"/>
        <rect x="520" y="280" width="40" height="30"/>
        <rect x="580" y="280" width="40" height="30"/>
        <rect x="640" y="280" width="40" height="30"/>
        <rect x="520" y="330" width="40" height="30"/>
        <rect x="580" y="330" width="40" height="30"/>
        <rect x="640" y="330" width="40" height="30"/>
      </g>
      
      <!-- Podium windows/storefronts -->
      <rect x="150" y="360" width="50" height="30"/>
      <rect x="250" y="360" width="50" height="30"/>
      <rect x="350" y="360" width="50" height="30"/>
      <rect x="450" y="360" width="50" height="30"/>
      <rect x="550" y="360" width="50" height="30"/>
    </g>
    
    <!-- Green roof elements -->
    <rect x="100" y="80" width="150" height="20" fill="#27ae60" opacity="0.8"/>
    <rect x="500" y="180" width="200" height="20" fill="#27ae60" opacity="0.8"/>
    
    <!-- Solar panels on tallest tower -->
    <g fill="#34495e" opacity="0.8">
      <rect x="310" y="10" width="50" height="10"/>
      <rect x="370" y="10" width="50" height="10"/>
    </g>
  </g>
  
  <!-- Urban context -->
  <g transform="translate(0, 450)">
    <!-- Plaza -->
    <rect width="800" height="150" fill="#95a5a6" opacity="0.8"/>
    
    <!-- Landscaping -->
    <g fill="#27ae60" opacity="0.8">
      <circle cx="100" cy="50" r="30"/>
      <circle cx="300" cy="70" r="25"/>
      <circle cx="500" cy="50" r="30"/>
      <circle cx="700" cy="70" r="25"/>
    </g>
    
    <!-- Water feature -->
    <ellipse cx="400" cy="70" rx="150" ry="30" fill="#3498db" opacity="0.6"/>
    
    <!-- Benches -->
    <rect x="150" y="100" width="50" height="10" fill="#7f8c8d"/>
    <rect x="350" y="100" width="50" height="10" fill="#7f8c8d"/>
    <rect x="550" y="100" width="50" height="10" fill="#7f8c8d"/>
  </g>
  
  <!-- Text label -->
  <g transform="translate(400, 550)" text-anchor="middle">
    <text font-family="Arial" font-size="24" fill="white" font-weight="bold">Urban Mixed-Use Development</text>
  </g>
</svg>