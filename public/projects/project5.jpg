<svg xmlns="http://www.w3.org/2000/svg" width="800" height="600" viewBox="0 0 800 600">
  <defs>
    <linearGradient id="grad1" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#8e44ad;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2c3e50;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="800" height="600" fill="url(#grad1)"/>
  
  <!-- Historic Renovation -->
  <g transform="translate(100, 100)">
    <!-- Main structure - historic building -->
    <g opacity="0.9">
      <!-- Building facade -->
      <rect x="0" y="0" width="600" height="350" fill="#e8d0b3"/>
      
      <!-- Roof -->
      <path d="M0,0 L300,-50 L600,0 Z" fill="#c0392b"/>
      
      <!-- Modern glass addition -->
      <rect x="400" y="100" width="150" height="250" fill="white" opacity="0.7"/>
      <rect x="410" y="110" width="130" height="230" fill="#3498db" opacity="0.7"/>
    </g>
    
    <!-- Historic details -->
    <g fill="#7f8c8d">
      <!-- Decorative cornice -->
      <rect x="0" y="50" width="600" height="10"/>
      <rect x="0" y="350" width="600" height="10"/>
      
      <!-- Columns -->
      <rect x="50" y="60" width="20" height="290"/>
      <rect x="150" y="60" width="20" height="290"/>
      <rect x="250" y="60" width="20" height="290"/>
      <rect x="350" y="60" width="20" height="290"/>
      
      <!-- Column capitals -->
      <rect x="40" y="60" width="40" height="20"/>
      <rect x="140" y="60" width="40" height="20"/>
      <rect x="240" y="60" width="40" height="20"/>
      <rect x="340" y="60" width="40" height="20"/>
      
      <!-- Column bases -->
      <rect x="40" y="330" width="40" height="20"/>
      <rect x="140" y="330" width="40" height="20"/>
      <rect x="240" y="330" width="40" height="20"/>
      <rect x="340" y="330" width="40" height="20"/>
    </g>
    
    <!-- Windows and doors -->
    <g>
      <!-- Historic arched windows -->
      <g fill="#3498db" opacity="0.7">
        <path d="M70,120 L120,120 L120,220 Q95,240 70,220 Z"/>
        <path d="M170,120 L220,120 L220,220 Q195,240 170,220 Z"/>
        <path d="M270,120 L320,120 L320,220 Q295,240 270,220 Z"/>
      </g>
      
      <!-- Main entrance -->
      <path d="M170,250 L270,250 L270,350 L170,350 Z" fill="#7f8c8d"/>
      <path d="M180,260 L260,260 L260,350 L180,350 Z" fill="#34495e"/>
      
      <!-- Door handles -->
      <circle cx="200" cy="300" r="5" fill="#f1c40f"/>
      <circle cx="240" cy="300" r="5" fill="#f1c40f"/>
    </g>
  </g>
  
  <!-- Urban context -->
  <g transform="translate(0, 450)">
    <!-- Sidewalk -->
    <rect width="800" height="150" fill="#95a5a6" opacity="0.8"/>
    
    <!-- Street lamps -->
    <g transform="translate(50, 0)">
      <rect x="0" y="0" width="5" height="70" fill="#34495e"/>
      <circle cx="2.5" cy="0" r="10" fill="#f1c40f" opacity="0.8"/>
    </g>
    
    <g transform="translate(250, 0)">
      <rect x="0" y="0" width="5" height="70" fill="#34495e"/>
      <circle cx="2.5" cy="0" r="10" fill="#f1c40f" opacity="0.8"/>
    </g>
    
    <g transform="translate(550, 0)">
      <rect x="0" y="0" width="5" height="70" fill="#34495e"/>
      <circle cx="2.5" cy="0" r="10" fill="#f1c40f" opacity="0.8"/>
    </g>
    
    <g transform="translate(750, 0)">
      <rect x="0" y="0" width="5" height="70" fill="#34495e"/>
      <circle cx="2.5" cy="0" r="10" fill="#f1c40f" opacity="0.8"/>
    </g>
  </g>
  
  <!-- Text label -->
  <g transform="translate(400, 550)" text-anchor="middle">
    <text font-family="Arial" font-size="24" fill="white" font-weight="bold">Historic Renovation</text>
  </g>
</svg>