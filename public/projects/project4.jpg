<svg xmlns="http://www.w3.org/2000/svg" width="800" height="600" viewBox="0 0 800 600">
  <defs>
    <linearGradient id="grad1" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#000428;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#004e92;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="800" height="600" fill="url(#grad1)"/>
  
  <!-- Sustainable Eco-Resort -->
  <g transform="translate(100, 150)">
    <!-- Main structures - multiple connected pavilions -->
    <g opacity="0.9">
      <!-- Central pavilion -->
      <rect x="200" y="100" width="200" height="150" rx="10" fill="white"/>
      
      <!-- Left pavilion -->
      <rect x="50" y="150" width="120" height="100" rx="10" fill="white"/>
      
      <!-- Right pavilion -->
      <rect x="430" y="150" width="120" height="100" rx="10" fill="white"/>
      
      <!-- Connecting walkways -->
      <rect x="170" y="180" width="30" height="40" fill="white"/>
      <rect x="400" y="180" width="30" height="40" fill="white"/>
      
      <!-- Roofs (slightly darker to show dimension) -->
      <path d="M200,100 L300,50 L400,100 Z" fill="#ecf0f1"/>
      <path d="M50,150 L110,120 L170,150 Z" fill="#ecf0f1"/>
      <path d="M430,150 L490,120 L550,150 Z" fill="#ecf0f1"/>
    </g>
    
    <!-- Solar panels -->
    <g opacity="0.8">
      <rect x="220" y="60" width="30" height="40" fill="#3498db"/>
      <rect x="260" y="60" width="30" height="40" fill="#3498db"/>
      <rect x="300" y="60" width="30" height="40" fill="#3498db"/>
      <rect x="340" y="60" width="30" height="40" fill="#3498db"/>
    </g>
    
    <!-- Windows -->
    <rect x="230" y="150" width="40" height="40" rx="5" fill="#3498db" opacity="0.7"/>
    <rect x="330" y="150" width="40" height="40" rx="5" fill="#3498db" opacity="0.7"/>
    <rect x="280" y="200" width="40" height="50" rx="5" fill="#3498db" opacity="0.7"/>
    
    <rect x="80" y="170" width="30" height="30" rx="5" fill="#3498db" opacity="0.7"/>
    <rect x="120" y="170" width="30" height="30" rx="5" fill="#3498db" opacity="0.7"/>
    
    <rect x="450" y="170" width="30" height="30" rx="5" fill="#3498db" opacity="0.7"/>
    <rect x="490" y="170" width="30" height="30" rx="5" fill="#3498db" opacity="0.7"/>
  </g>
  
  <!-- Natural environment -->
  <g transform="translate(0, 400)">
    <!-- Water -->
    <rect width="800" height="200" fill="#3498db" opacity="0.5"/>
    
    <!-- Beach -->
    <path d="M0,0 L800,0 L800,50 C600,30 200,30 0,50 Z" fill="#f1c40f" opacity="0.7"/>
    
    <!-- Palm trees -->
    <g transform="translate(50, 0)">
      <rect x="0" y="-20" width="10" height="50" fill="#795548" transform="rotate(15)"/>
      <path d="M0,-20 C10,-40 20,-30 30,-40 C20,-20 30,-10 20,0 C10,-10 0,-20 0,-20 Z" fill="#27ae60"/>
    </g>
    
    <g transform="translate(700, 0)">
      <rect x="0" y="-20" width="10" height="50" fill="#795548" transform="rotate(-15)"/>
      <path d="M0,-20 C-10,-40 -20,-30 -30,-40 C-20,-20 -30,-10 -20,0 C-10,-10 0,-20 0,-20 Z" fill="#27ae60"/>
    </g>
    
    <!-- Waves -->
    <path d="M0,100 Q100,80 200,100 Q300,120 400,100 Q500,80 600,100 Q700,120 800,100 L800,200 L0,200 Z" fill="#2980b9" opacity="0.3"/>
  </g>
  
  <!-- Text label -->
  <g transform="translate(400, 550)" text-anchor="middle">
    <text font-family="Arial" font-size="24" fill="white" font-weight="bold">Sustainable Eco-Resort</text>
  </g>
</svg>