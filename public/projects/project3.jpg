<svg xmlns="http://www.w3.org/2000/svg" width="800" height="600" viewBox="0 0 800 600">
  <defs>
    <linearGradient id="grad1" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1a2a6c;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#b21f1f;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#fdbb2d;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="800" height="600" fill="url(#grad1)"/>
  
  <!-- Cultural Center -->
  <g transform="translate(100, 100)">
    <!-- Main structure - curved roof design -->
    <path d="M0,200 Q300,-100 600,200 L600,400 L0,400 Z" fill="white" opacity="0.9"/>
    
    <!-- Glass entrance -->
    <rect x="200" y="300" width="200" height="100" fill="#3498db" opacity="0.8"/>
    
    <!-- Decorative elements -->
    <path d="M100,200 Q300,50 500,200" fill="none" stroke="#7f8c8d" stroke-width="5"/>
    <path d="M150,200 Q300,80 450,200" fill="none" stroke="#7f8c8d" stroke-width="5"/>
    <path d="M200,200 Q300,100 400,200" fill="none" stroke="#7f8c8d" stroke-width="5"/>
    
    <!-- Windows -->
    <rect x="50" y="250" width="80" height="80" rx="10" fill="#3498db" opacity="0.7"/>
    <rect x="150" y="250" width="80" height="80" rx="10" fill="#3498db" opacity="0.7"/>
    <rect x="370" y="250" width="80" height="80" rx="10" fill="#3498db" opacity="0.7"/>
    <rect x="470" y="250" width="80" height="80" rx="10" fill="#3498db" opacity="0.7"/>
    
    <!-- Columns -->
    <rect x="50" y="330" width="10" height="70" fill="#7f8c8d"/>
    <rect x="150" y="330" width="10" height="70" fill="#7f8c8d"/>
    <rect x="250" y="330" width="10" height="70" fill="#7f8c8d"/>
    <rect x="350" y="330" width="10" height="70" fill="#7f8c8d"/>
    <rect x="450" y="330" width="10" height="70" fill="#7f8c8d"/>
    <rect x="550" y="330" width="10" height="70" fill="#7f8c8d"/>
  </g>
  
  <!-- Landscape elements -->
  <g transform="translate(0, 500)">
    <!-- Ground -->
    <rect width="800" height="100" fill="#27ae60" opacity="0.7"/>
    
    <!-- Pathway -->
    <path d="M300,0 L500,0 L550,100 L250,100 Z" fill="#bdc3c7" opacity="0.8"/>
    
    <!-- Water feature -->
    <ellipse cx="150" cy="50" rx="100" ry="30" fill="#3498db" opacity="0.6"/>
    
    <!-- Decorative plants -->
    <circle cx="50" cy="30" r="20" fill="#16a085" opacity="0.8"/>
    <circle cx="650" cy="30" r="20" fill="#16a085" opacity="0.8"/>
    <circle cx="700" cy="50" r="15" fill="#16a085" opacity="0.8"/>
    <circle cx="750" cy="30" r="20" fill="#16a085" opacity="0.8"/>
  </g>
  
  <!-- Text label -->
  <g transform="translate(400, 550)" text-anchor="middle">
    <text font-family="Arial" font-size="24" fill="white" font-weight="bold">Cultural Arts Center</text>
  </g>
</svg>